<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- 별칭(별명) 설정 -->
	<typeAliases>
		<typeAlias alias="product" type="model.product.ProductVO" />
		<typeAlias alias="member" type="model.member.MemberVO" />
		<typeAlias alias="history" type="model.history.HistoryVO" />
	</typeAliases>

	<!-- mappings패키지안의 메서드(==설정파일)와 연결 -->
	<mappers>
		<mapper resource="mappings/product-mapping.xml" />
		<mapper resource="mappings/member-mapping.xml" />
		<mapper resource="mappings/history-mapping.xml" />
	</mappers>


</configuration>